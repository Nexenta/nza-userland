set name=pkg.fmri value=pkg:/terminal/dialog@$(PKGVERS)
set name=info.classification value=org.opensolaris.category.2008:Applications/System Utilities
set name=variant.arch value=$(ARCH)
set name=variant.opensolaris.zone value=global value=nonglobal
set name=description value="Dialog"
set name=pkg.description value="Dialog"
set name=pkg.summary value="Dialog"
dir path=usr
dir path=usr/bin
dir path=usr/share
dir path=usr/share/man
dir path=usr/share/man/man1
file path=usr/bin/dialog mode=0555
file path=usr/share/man/man1/dialog.1
depend fmri=library-ncurses@$(PKGVERS) type=require
depend fmri=system-library/math@$(PKGVERS) type=require
depend fmri=system-library@$(PKGVERS) type=require

