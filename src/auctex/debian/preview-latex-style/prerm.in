#include "variables"
#! /bin/bash -e
#
# <% postinst %> script for <:=${PLSPKG}:>
#
<:=@COPYRIGHT:>//

set -o posix

case "${1}" in
    deconfigure|failed-upgrade|remove|upgrade) :;;
    *) echo >&2 "${0##*/}/<:=${PLSPKG}:>:" \
	"Unknown argument: \"${1}\"."
	exit 1
esac

exit 0
