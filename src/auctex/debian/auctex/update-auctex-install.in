#include "variables"
#! /bin/bash -e
#
# Update AUCTeX installation.
# Installed by the Debian <:=${PACKAGE}:> package.
#
<:=@COPYRIGHT:>//

set -o posix

<:# Default flavors: only GNU, XEmacs comes with its own AUCTeX.:>//
FLAVORS=${*:-"emacs21 emacs22 emacs23 emacs-snapshot"}

for FLAVOR in ${FLAVORS}; do
    if [ -x /usr/bin/${FLAVOR} ]; then
	<:=${ecode}:>/remove/<:=${PACKAGE}:> ${FLAVOR}
	<:=${ecode}:>/install/<:=${PACKAGE}:> ${FLAVOR}
    fi
done

<:
# local variables:
# mode: shell-script
# ispell-local-dictionary: "american"
# ispell-check-comments: exclusive
# end:
#
# LocalWords:  AUCTeX debconf
:>//
