diff -urpN python2.5-2.5.2.old/configure.in python2.5-2.5.2/configure.in
--- python2.5-2.5.2.old/configure.in	2008-02-13 19:17:17.000000000 +0000
+++ python2.5-2.5.2/configure.in	2008-09-26 21:00:02.000000000 +0100
@@ -179,6 +179,17 @@ then
 	MACHDEP="$ac_md_system$ac_md_release"
 
 	case $MACHDEP in
+	linux2*)
+	    ac_sys_machine=`uname -m`
+	    ac_md_machine=`echo $ac_sys_machine |
+				tr -d '[/ ]' | tr '[[A-Z]]' '[[a-z]]'`
+	    case $ac_md_machine in
+	    alpha*) MACHDEP="$MACHDEP-alpha" ;;
+	    parisc*) MACHDEP="$MACHDEP-hppa" ;;
+	    mips*) MACHDEP="$MACHDEP-mips" ;;
+	    sparc*) MACHDEP="$MACHDEP-sparc" ;;
+	    esac
+	    ;;
 	cygwin*) MACHDEP="cygwin";;
 	darwin*) MACHDEP="darwin";;
 	atheos*) MACHDEP="atheos";;
