#!/bin/sh -e

case "$1" in
	remove)
		;;
        purge)
                update-rc.d hwclock.sh remove >/dev/null || true
		update-rc.d hwclockfirst.sh remove >/dev/null || true
		rm -f /etc/adjtime
                ;;
	*)
		;;
esac

if [ -x /usr/sbin/update-mime ]; then
	update-mime
fi


