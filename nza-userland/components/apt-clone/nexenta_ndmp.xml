<service_bundle type='profile' name='default'>

<!-- Name the service to 'system/ndmpd'  -->
<service name='system/ndmpd' type='service' version='1'>

        <property_group name='ndmpd' type='application'>
                <propval name='vendor-name' type='astring' value='Nexenta' />
        </property_group>
        <property_group name='ndmpd' type='application'>
                <propval name='product-name' type='astring' value='NexentaStor' />
        </property_group>

</service>

</service_bundle>

